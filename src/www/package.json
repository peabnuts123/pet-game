{
  "private": true,
  "name": "pet-game",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "scripts": {
    "start": "preact watch",
    "simulate-build": "npm run test && npm run lint && npm run type-check && npm run build && echo 'Build succeeded'",
    "build": "rm -rf build && preact build --dest build && npm run export-routes && cp export-routes/route-map.json build/",
    "lint": "eslint --max-warnings 0 --no-error-on-unmatched-pattern \"./*.{js,jsx,ts,tsx}\" \"src/**/*.{js,jsx,ts,tsx}\" \"test/**/*.{js,jsx,ts,tsx}\"",
    "type-check": "tsc --project ./tsconfig.json",
    "test": "jest",
    "test:coverage": "jest --collectCoverage true && serve --config serve.coverage.json --no-clipboard -l 8081",
    "export-routes": "ts-node -r tsconfig-paths/register --project export-routes/tsconfig.json export-routes/export-routes.ts"
  },
  "dependencies": {
    "preact": "^10.3.1",
    "preact-router": "^3.2.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/preact": "^2.0.0",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^26.0.8",
    "@types/react": "^16.9.49",
    "@types/webpack-env": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "classnames": "^2.2.6",
    "eslint": "^7.7.0",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "jest": "^26.2.2",
    "jest-fetch-mock": "^3.0.3",
    "jest-preset-preact": "^4.0.2",
    "mobx": "^5.15.6",
    "mobx-react-lite": "^2.1.0",
    "normalize.css": "^8.0.1",
    "preact-cli": "^3.0.0",
    "preact-render-to-string": "^5.1.10",
    "react-feather": "^2.0.8",
    "sass": "^1.26.10",
    "sass-loader": "^10.0.1",
    "serve": "^11.3.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.2"
  }
}
